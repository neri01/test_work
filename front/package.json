{
  "name": "cardiosphera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint && yarn lint:styles",
    "lint:styles": "stylelint \"src/**/*.{scss,vue}\"",
    "lint:fix": "eslint --fix --ext js,ts,vue src && yarn lint:styles --fix"
  },
  "dependencies": {
    "ajax": "^0.0.4",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
    "jquery": "^3.6.0",
    "md5-js": "^0.0.3",
    "mobile-device-detect": "^0.4.3",
    "simple-vue-validator": "^0.16.0",
    "smoothscroll-polyfill": "^0.4.4",
    "stylelint-config-standard": "^21.0.0",
    "swiper": "5.0.3",
    "v-mask": "^2.2.3",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-cli": "^2.9.6",
    "vue-click-outside": "^1.1.0",
    "vue-cookies": "^1.7.4",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.2.0",
    "vue-scrollto": "^2.20.0",
    "vue-slick-carousel": "^1.0.6",
    "vue-svg-loader": "^0.16.0",
    "vue-vimeo-player": "^0.1.1",
    "vue-yandex-maps": "^0.10.9",
    "vue2-collapse": "^1.0.15",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.5.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/vue": "^6.0.21",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.1.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-prettier": "^1.1.2",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "vue-cli-service lint",
      "prettier --write",
      "git add"
    ]
  }
}
